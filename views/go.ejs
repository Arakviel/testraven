<body>
  <div class="demo-2">
    <div id="large-header" class="large-header">
      <canvas id="demo-canvas"></canvas>
      <div class="col-lg-6 col-lg-offset-3 text-center test1">
        <div class="alert alert-info">
          <a href="/" class="btn btn-info">На головну</a>
          <p><span class="label label-info"><%=data.name%> на питанні:<%= count %>/60</span></p>
        </div>
      </div>
    </div>
    <div class="col-lg-6 col-lg-offset-3 test2">
      <img src="<%= picture %>" class="img-thumbnail"/>
    </div>
    <div class="col-lg-10 text-center test3">
      <br>
      <div class="alert alert-info text-center">
        <p><span class="label label-info">Виберіть правильний:</span></p>
        <br>
        <form class="" method="post" action="/test/go">

          <ul class="list-inline">
            <li class="list-group-item"><button id="a1" class="list-group-item list-group-item-action" name="a" value="1"></button></li>
            <li class="list-group-item"><button id="a2" class="list-group-item list-group-item-action" name="a" value="2"></button></li>
